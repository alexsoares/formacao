        <table>
            <tr>
             <td>  <b><%= f.label :Nome %></b></td>
             <td>  <%= f.text_field :Nome %></td>
           </tr>
           <tr>
             <td> <b>Trabalho</b></td>
             <td> <%= f.text_field :Trabalho %></td>
           </tr>
